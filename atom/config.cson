"*":
  "autocomplete-plus":
    autoActivationDelay: 500
    confirmCompletion: "tab always, enter when suggestion explicitly selected"
  core:
    audioBeep: false
    automaticallyUpdate: false
    disabledPackages: [
      "language-clojure"
      "hasklig"
      "firacode"
      "minimap"
      "wrap-guide"
    ]
    packagesWithKeymapsDisabled: [
      "split-diff"
    ]
    telemetryConsent: "limited"
    titleBar: "custom"
  editor:
    fontFamily: "FiraCode-Light"
    invisibles: {}
    scrollPastEnd: true
  "elm-format": {}
  elmjutsu:
    cacheDirectory: "/var/folders/8k/2zxgz_6954z69h869gjhk86r0000gn/T/elmjutsu29999cdd6WKGfNuxe"
  "exception-reporting":
    userId: "d119ab13-dece-4547-a40c-d52c19fda4f2"
  firacode:
    fontWeight: "light"
  "git-blame":
    columnWidth: 300
  "git-plus":
    commits:
      verboseCommits: true
    experimental:
      useSplitDiff: true
    general:
      splitPane: "Right"
    remoteInteractions:
      pullBeforePush: true
      pullRebase: true
  "linter-ui-default":
    panelHeight: 69
  "markdown-preview": {}
  "markdown-toc-auto":
    max: 5
  minimap: {}
  "no-title-bar":
    enableLiberalWindowDragging: false
  pigments:
    markerType: "native-square-dot"
  "recent-files-fuzzy-finder":
    restoreSession: true
  "split-diff":
    ignoreWhitespace: true
    turnOffSoftWrap: true
  "vim-mode-plus-ex-mode":
    notifiedUseExMode: true
  welcome:
    showOnStartup: false
  whitespace:
    ignoreWhitespaceOnCurrentLine: false
