alias aliases='vim ~/.dotfiles/zsh/aliases'
alias amend='git commit --amend --no-edit'
alias c='clear'
alias choose='choose.rb'
alias colors='colors.sh'
alias colors_short='colors_short.sh'
alias ef='exercism fetch'
alias es='exercism submit'
alias hidefiles='defaults write com.apple.finder AppleShowAllFiles NO; killall Finder /System/Library/CoreServices/Finder.app'
alias notes='cd ~/Dropbox/Notes'
alias path='echo $PATH | tr -s ":" "\n"'
alias showfiles='defaults write com.apple.finder AppleShowAllFiles YES; killall Finder /System/Library/CoreServices/Finder.app'
alias soa='source ~/.dotfiles/zsh/aliases'
alias ta='tmux a -t'
alias tk='tmux kill-session -t'
alias tks='tmux kill-session -t'
alias tl='tmux ls'
alias tmux="TERM=screen-256color-bce tmux"
alias tn='tmux new -s'
alias v='vim'
alias vs='ls -al ~/.vim/sessions'
alias vushuru='cd ~/Development/vzaar/vzaar-ushuru'
alias vz='mux vz'
